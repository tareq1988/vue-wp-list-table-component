{
  "name": "@rebelcode/vue-wp-list-table",
  "description": "WordPress List Table component for Vue.js",
  "version": "1.1.4",
  "author": {
    "name": "Tareq Hasan",
    "email": "tareq@wedevs.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin"
  ],
  "license": "MIT",
  "main": "dist/vue-wp-list-table.common.js",
  "unpkg": "dist/vue-wp-list-table.browser.js",
  "module": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "dev:build": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --hide-modules",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tareq1988/vue-wp-list-table-component.git"
  },
  "bugs": {
    "url": "https://github.com/tareq1988/vue-wp-list-table-component/issues"
  },
  "homepage": "https://github.com/tareq1988/vue-wp-list-table-component#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.6.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^2.1.0-beta.28",
    "webpack-merge": "^1.1.2"
  }
}
